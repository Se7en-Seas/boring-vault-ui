{
  "name": "boring-vault-ui",
  "version": "1.9.12",
  "description": "A reusable package to quickly integrate boring vaults onto a UI.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/types/index.d.ts",
    "./solana": {
      "import": "./dist/solana/sdk/index.js",
      "require": "./dist/solana/sdk/index.js",
      "types": "./dist/solana/sdk/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:sui": "cd src/sui && tsc",
    "prepublish": "npm run build",
    "test": "jest --testPathIgnorePatterns src/sui",
    "dev": "webpack --mode production && webpack serve --mode development --open",
    "type:check": "tsc --noEmit",
    "type:check:sui": "cd src/sui && tsc --noEmit",
    "test:solana": "ts-node src/solana/tests/solana-sdk-unit-tests.ts",
    "test:solana-queue": "ts-node src/solana/tests/queue-unit-tests.ts",
    "mainnet-test-solana": "ts-node src/solana/tests/mainnet-test.ts",
    "test:sui": "jest --config src/sui/jest.config.js"
  },
  "keywords": [],
  "author": "Philip Bal",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@coral-xyz/anchor": "^0.31.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@ethersproject/bytes": "^5.8.0",
    "@layerzerolabs/lz-definitions": "^3.0.127",
    "@layerzerolabs/lz-sdk": "^0.0.87",
    "@mysten/bcs": "^1.6.3",
    "@mysten/sui": "^1.34.0",
    "@pythnetwork/hermes-client": "^2.0.0",
    "@pythnetwork/pyth-solana-receiver": "^0.10.1",
    "@safe-global/safe-apps-provider": "^0.18.3",
    "@safe-global/safe-apps-sdk": "^9.1.0",
    "@solana/spl-token": "^0.4.13",
    "@switchboard-xyz/common": "^3.3.0",
    "@switchboard-xyz/on-demand": "^2.10.4",
    "@wagmi/connectors": "^5.7.13",
    "@wagmi/core": "^2.17.0",
    "dotenv": "^16.6.0",
    "ethers": "^6.13.7",
    "framer-motion": "^11.0.27",
    "gill": "^0.9.0",
    "next": "^14.2.28",
    "react": "^18.2.0",
    "viem": "^2.21.2",
    "wagmi": "^2.15.0"
  },
  "overrides": {
    "@coral-xyz/anchor": {
      "@solana/web3.js": "^1.98.0"
    },
    "@pythnetwork/pyth-solana-receiver": {
      "@solana/web3.js": "^1.77.0"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@typemove/move": "^1.9.0",
    "@typemove/sui": "^1.9.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "babel-loader": "^9.1.3",
    "bignumber.js": "^9.1.2",
    "connectkit": "^1.7.3",
    "dotenv-types": "^1.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "typesVersions": {
    "*": {
      "solana": [
        "dist/solana/sdk/index.d.ts"
      ]
    }
  }
}
